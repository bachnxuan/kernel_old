# Camera
CONFIG_CUSTOM_KERNEL_IMGSENSOR="platos5khm6_mipi_raw platoimx596_mipi_raw platogc02m1_mipi_raw platos5k4h7_mipi_raw"

# Charger
# CONFIG_MTK_PUMP_EXPRESS_PLUS_SUPPORT is not set
# CONFIG_MTK_PUMP_EXPRESS_PLUS_20_SUPPORT is not set
# CONFIG_MTK_PUMP_EXPRESS_40_SUPPORT is not set
# CONFIG_MTK_PUMP_EXPRESS_50_SUPPORT is not set
# CONFIG_GAUGE_MT6375 is not set
CONFIG_REGULATOR_WL2868C=m
CONFIG_REGULATOR_WL2866D=m

# Display
CONFIG_DRM_PANEL_L12A_36_02_0B_DSC_CMD=m
CONFIG_DRM_PANEL_L12A_42_02_0A_DSC_CMD=m
CONFIG_MI_DISP_DSC2712=y
CONFIG_MI_DISP_ESD_CHECK=y

# Fingerprint
CONFIG_MTK_FINGERPRINT_SUPPORT=m
# CONFIG_GOODIX_CAP_FINGERPRINT is not set
# CONFIG_FPC_FINGERPRINT is not set
CONFIG_GOODIX_FINGERPRINT=m

# Thermal
CONFIG_MI_THERMAL_MULTI_CHARGE=y

# Touchscreen
CONFIG_TOUCHSCREEN_MTK_TOUCH="GT9886 GT9896S NT36672C"
CONFIG_TOUCHSCREEN_XIAOMI_TOUCHFEATURE=m
CONFIG_TOUCHSCREEN_ST_FTS_V521_SPI_L12A=m
CONFIG_TOUCHSCREEN_GOODIX_BRL_9916=m
CONFIG_TOUCHSCREEN_NVT36672C_DOUBLE_CLICK=m

# Product
CONFIG_BUILD_PRODUCT_NAME="plato"
CONFIG_TARGET_PRODUCT_PLATO=y

# Sound
CONFIG_SND_SOC_CS35L41=m
CONFIG_MTK_VOW_MAX_PDK_NUMBER=0
CONFIG_MTK_VOW_AMAZON_SUPPORT=n
CONFIG_MTK_VOW_GVA_SUPPORT=y

# TEE
CONFIG_TRUSTONIC_TRUSTED_UI=m
