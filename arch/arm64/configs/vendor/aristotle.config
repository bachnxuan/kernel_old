# Camera
CONFIG_CUSTOM_KERNEL_IMGSENSOR="aristotleimx707_mipi_raw aristotleimx596_mipi_raw aristotleov50d_mipi_raw aristotleov13b10_mipi_raw"

# Charger
CONFIG_MTK_PUMP_EXPRESS_PLUS_SUPPORT=m
CONFIG_MTK_PUMP_EXPRESS_PLUS_20_SUPPORT=m
CONFIG_MTK_PUMP_EXPRESS_40_SUPPORT=m
CONFIG_MTK_PUMP_EXPRESS_50_SUPPORT=m
CONFIG_GAUGE_MT6375=m
CONFIG_XMEXT_LN8000_CHG_PUMP=m
CONFIG_REGULATOR_WL2868C=m

# Display
CONFIG_DRM_PANEL_M12A_36_02_0A_DSC_CMD=m
CONFIG_MI_DISP_DSC2712=y

# Fingerprint
CONFIG_MTK_FINGERPRINT_SUPPORT=m
CONFIG_GOODIX_MITEE_FINGERPRINT=m

# Thermal
CONFIG_MI_THERMAL_MULTI_CHARGE=y

# Touchscreen
# CONFIG_TOUCHSCREEN_MTK_TOUCH is not set
CONFIG_TOUCHSCREEN_XIAOMI_TOUCHFEATURE=m
CONFIG_TOUCHSCREEN_GOODIX_BRL_9916=m

# Product
CONFIG_BUILD_PRODUCT_NAME="aristotle"
CONFIG_TARGET_PRODUCT_ARISTOTLE=y

# Sound
CONFIG_SND_SOC_TFA9874=m
CONFIG_MTK_VOW_MAX_PDK_NUMBER=0
CONFIG_MTK_VOW_AMAZON_SUPPORT=n
CONFIG_MTK_VOW_GVA_SUPPORT=y
CONFIG_MTK_VOW_BARGE_IN_SUPPORT=y
CONFIG_MTK_VOW_DUAL_MIC_SUPPORT=y
CONFIG_MTK_VOW_PCMCALLBACK_SUPPORT=n
CONFIG_SND_SMARTPA_AW882XX_V11=m
CONFIG_SND_SMARTPA_AW882XX_GPIO_CONFIG_V11=m

# TEE
CONFIG_MITEE_SUPPORT=m
